{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "test": "vitest run",
    "ncu": "ncu -i --format group",
    "gen-db-types": "npx supabase gen types typescript --project-id ononpdjuwjkqsfewrtoy > ./app/schemas/supabase/database.d.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write",
      "cspell ."
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.1",
    "vee-validate": "^5.0.0-beta.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@cspell/dict-pt-br": "^2.4.2",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/supabase": "^1.6.2",
    "@pinia/nuxt": "^0.11.2",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^13.9.0",
    "cspell": "^9.2.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "playwright-core": "^1.55.0",
    "prettier": "^3.6.2",
    "supabase": "^2.40.7",
    "vitest": "^3.2.4",
    "vuetify-nuxt-module": "^0.18.7"
  }
}
